{
  "manifest_version": 2,
  "name": "Quora with MathJax",
  "version": "0.0.1",
  "description": "Renders Quora equations with MathJax",
  "content_scripts": [
    {
      "matches": ["http://www.quora.com/*"],
      "js": ["jquery-min-1.7.2.js", "content.js"],
      "run_at": "document_end"
    }
  ]
}
